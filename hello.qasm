OPENQASM 2.0;

include "qelib1.inc";

qreg q[1];

// เริ่มต้นวงจร
h q[0];

// พิมพ์ข้อความ "สวัสดี, ชาวโลก!"
gate hello(q) {
  x q;
  s q;
  x q;
  cnot q[0], q[0];
}

// เรียกใช้ประตู hello()
hello q[0];

// แสดงผลลัพธ์
measure q[0] -> c[0];

// ตรวจสอบผลลัพธ์
if (c[0] == 1) {
  // พิมพ์ "สวัสดี, ชาวโลก!"
  print("สวัสดี, ชาวโลก!");
}